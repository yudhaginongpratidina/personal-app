{
    "name": "backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "make": "ts-node -r tsconfig-paths/register src/cli/make.ts",
        "delete": "ts-node -r tsconfig-paths/register src/cli/make.ts",
        "dev": "nodemon",
        "test": "jest",
        "build": "tsc && tsc-alias",
        "start": "node -r tsconfig-paths/register dist/index.js"
    },
    "jest": {
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest"
        },
        "verbose": true,
        "collectCoverage": false,
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100,
                "statements": 100
            }
        },
        "collectCoverageFrom": [
            "!**/node_modules/**",
            "!index.js",
            "!index.ts",
            "!**/dist/**"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.6.5",
    "dependencies": {
        "@prisma/client": "^6.6.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "kleur": "^4.1.5",
        "winston": "^3.17.0",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-typescript": "^7.27.0",
        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.1",
        "@types/jest": "^29.5.14",
        "@types/supertest": "^6.0.3",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "prisma": "^6.6.0",
        "supertest": "^7.1.0",
        "ts-jest": "^29.3.2",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.15",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.3"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@prisma/client",
            "@prisma/engines",
            "esbuild",
            "prisma"
        ]
    }
}