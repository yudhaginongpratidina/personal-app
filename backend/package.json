        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "express-rate-limit": "^7.5.0",
        "jsonwebtoken": "^9.0.2",
        "kleur": "^4.1.5",
        "winston": "^3.17.0",
        "zod": "^3.24.2"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@prisma/client",
